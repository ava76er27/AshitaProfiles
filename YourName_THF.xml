			<body>Meg. Cuirie +1</body>
			<hands>Mummu Wrists +2</hands>
			<legs augment="S704721408">Samnuha Tights</legs>
			<feet>Meg. Jam. +1</feet>
			<neck>Sanctity Necklace</neck>
			<waist>Svelt. Gouriz +1</waist>
			<ear1>Phawaylla Earring</ear1>
