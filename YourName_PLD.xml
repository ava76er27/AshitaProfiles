				<setvar name="Gset" value="Hate" /><addtochat color="4">Gear $Gset</addtochat>			
			<equip set="Hate" />
		</cmd>
	</inputcommands>
	<idlegear>
<!---
	    <if advanced="$init=0">
				<addtochat color="$Color">$Buffer</addtochat>
				<addtochat color="$Color">ava76er27/Profiles: Palidan ver. 0.17</addtochat>
