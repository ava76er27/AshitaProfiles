	<inputcommands>
		<cmd input="/skillup">
			<equip set="Skillup" />
		</cmd>
		<cmd input="/pds">
			<equip set="PDS" />
		</cmd>
		<cmd input="/sword">
			<equip set="Swords" />
		</cmd>
		<cmd input="/great">
			<equip set="Great" />
		</cmd>
		<cmd input="/cp">
		<if advanced="$CP=0">
			<equip>
			<back>Aptitude Mantle</back>				
			</equip><gearlock delay="4" />
			<command>/ac disable back</command>
			<setvar name="CP" value="1" /><addtochat color="4">CP disable $CP</addtochat>
		</if>
		<else>
			<command>/ac enable back</command>
			<setvar name="CP" value="0" /><addtochat color="4">CP enable $CP</addtochat>
		</else>
		</cmd>
		<cmd input="/idle">
				<setvar name="Gear" value="0" />
				<setvar name="Mem" value="$Gear" />
				<setvar name="Gset" value="TP-Accuracy" /><addtochat color="4">Gear $Gset</addtochat>				
			<equip set="TP-Accuracy" />
		</cmd>	
		<cmd input="/pdt">
				<setvar name="Gear" value="1" />
				<setvar name="Mem" value="$Gear" />
				<setvar name="Gset" value="PDT" /><addtochat color="4">Gear $Gset</addtochat>			
			<equip set="PDT" />
		</cmd>
		<cmd input="/mdt">
				<setvar name="Gear" value="2" />
