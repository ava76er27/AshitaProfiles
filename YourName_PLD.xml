			<setvar name="init" value="1" />		
			<command>/exec avatarx/pld.txt</command>
			<setvar name="lockstyleset" value="06" />
			<if p_mainjob="PLD"><setvar name="macrobook" value="12" /></if>
			<setvar name="macropage" value="01" />

			<command delay="1500">/macro book $macrobook</command>
			<command delay="3000">/macro set $macropage</command>
			<command delay='6000'>/lockstyleset $lockstyleset</command>
		</if>
--->	
		<if p_status="engaged">
				<setvar name="toggle" value="1" />
				<setvar name="Gear" value="$Mem" />				
			<if advanced="$Gear=0"><equip set="Engaged" /><setvar name="Gset" value="Engaged" /></if>		
			<if advanced="$Gear=1"><equip set="PDT" /><setvar name="Gset" value="PDT" /></if>
			<if advanced="$Gear=2"><equip set="MDT" /><setvar name="Gset" value="MDT" /></if>
			<if advanced="$Gear=2"><equip set="Hate" /><setvar name="Gset" value="Hate" /></if>
		</if>
		<elseif>
				<setvar name="toggle" value="0" />
				<setvar name="mem" value="$Gear" />				
				<equip set="$Gset" />				
		</elseif>

		<if eq_rring="Facility Ring"><equip lock="true"><rring>Facility Ring</rring></equip></if>
		<if eq_rring="Trizek Ring"><equip lock="true"><rring>Trizek Ring</rring></equip></if>
